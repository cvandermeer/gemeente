<div class='inner'>
  <div class='container'>
    <h2>Nieuws van <%= @community.name %></h2>

    <% @newsletters.each do |newsletter| %>
      <p>
        <%= newsletter.community.name %> -
        <%= link_to newsletter.title, newsletter %>
        (<%= newsletter.valid_from %> -
        <%= newsletter.valid_until %>)
      </p>
    <% end %>
  </div>
</div>
