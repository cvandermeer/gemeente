<%- if controller_name != 'sessions' %>
  <%= link_to 'Inloggen', new_session_path(resource_name), class: 'js_modal', remote: true %>
<% end -%>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to 'Aanmelden', new_registration_path(resource_name), remote: true, class: 'js_modal' %>
<% end -%>
