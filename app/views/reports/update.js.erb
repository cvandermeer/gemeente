$('#edit_report_' + <%= @report.id %>).on('ajax:success', function() {
  $.ajax({
    type: 'GET',
    dataType: 'json',
    url: '/reports/markers',
    data: { 'id': <%= @report.id %> },
    success: function(data){
      goToReportLocation(data);
      removeSideModal($('.side-modal'));
      $('.new-marker').remove();
    }
  });
});
